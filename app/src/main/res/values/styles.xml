<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorCyan</item>
        <item name="colorPrimaryDark">@color/colorBlue</item>
        <item name="colorAccent">@color/colorOrange</item>

        <item name="android:textColor">@color/colorBlack</item>
        <item name="tabTextColor">@color/colorBlack</item>
        <item name="tabIndicatorColor">@color/colorOrange</item>

        <item name="bottemnav">@color/colorCyan</item>
        <item name="backgroundColor">@color/colorWhite</item>
        <item name="bottemnavitemclick">@color/colorBlue</item>
        <item name="bottemnavitem">@color/colorWhite</item>

        <item name="backgroundTint">@color/colorCyan</item>
        <item name="colorDark">@color/colorBlack</item>
        <item name="colorLight">@color/colorWhite</item>

        <item name="colorCyan">@color/colorCyan</item>
        <item name="colorBlue">@color/colorBlue</item>
        <item name="colorOrange">@color/colorOrange</item>

        <item name="colorGrey">@color/colorGrey</item>
        <item name="colorRed">@color/colorRed</item>
        <item name="colorGreen">@color/colorGreen</item>
    </style>

    <!-- Base application theme. -->
    <style name="DarkTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorBlue</item>
        <item name="colorPrimaryDark">@color/colorBlack</item>
        <item name="colorAccent">@color/colorOrange</item>

        <item name="android:textColor">@color/colorWhite</item>
        <item name="tabTextColor">@color/colorWhite</item>
        <item name="tabIndicatorColor">@color/colorOrange</item>

        <item name="bottemnav">@color/colorBlue</item>
        <item name="backgroundColor">@color/colorBlack</item>
        <item name="bottemnavitemclick">@color/colorCyan</item>
        <item name="bottemnavitem">@color/colorWhite</item>

        <item name="android:textColorPrimary">@color/colorWhite</item>
        <item name="titleTextColor">@color/colorWhite</item>
        <item name="backgroundTint">@color/colorBlue</item>

        <item name="colorDark">@color/colorGrey</item>
        <item name="colorLight">@color/colorBlack</item>
        <item name="colorCyan">@color/colorCyan</item>
        <item name="colorBlue">@color/colorBlue</item>
        <item name="colorOrange">@color/colorOrange</item>
        <item name="colorGrey">@color/colorGrey</item>
        <item name="colorRed">@color/colorRed</item>
        <item name="colorGreen">@color/colorGreen</item>

    </style>
</resources>